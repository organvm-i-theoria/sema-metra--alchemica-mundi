{
  "name": "sema-metra--alchemica-mundi",
  "version": "1.0.0",
  "description": "sēma-mētra--alchemica-mundi: Signal-Matrix for World-Alchemy",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "sema": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "data"
  ],
  "scripts": {
    "build": "tsup && cp -r data dist/",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "cli": "tsx src/cli/index.ts"
  },
  "keywords": [
    "matrix",
    "synthesis",
    "signal",
    "alchemy",
    "modulation",
    "patchbay"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.3.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "commander": "^14.0.3",
    "tone": "^15.0.4",
    "zod": "^4.3.6"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
